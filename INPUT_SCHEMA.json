    "postalCodes": {
      "title": "Postal codes",
      "type": "string",
      "description": "A whitespace-separated list of postal codes from which you need residential proxies. It cannot be used together with <b>DMA codes</b>.",
      "editor": "textarea"
    },
    "dmaCodes": {
      "title": "DMA codes",
      "type": "string",
      "description": "A whitespace-separated list of Designated Market Area (DMA) codes as defined by Nielsen from which you need residential proxies. It cannot be used together with <b>Postal codes</b>.",
      "prefill": "524 602",
      "editor": "textarea"
    },
    "maxSessions": {
      "title": "Max sessions",
      "type": "integer",
      "description": "Maximum number of residential proxy sessions to maintain and keep alive. The higher the value, the more residential proxy traffic the actor will consume!",
      "minimum": 1,
      "maximum": 9999,
      "default": 100
    },
    "minSessionsPerRegion": {
      "title": "Min sessions per region",
      "type": "integer",
      "description": "Minimum number of residential proxy sessions to maintain and keep alive for a single postal code or DMA (whichever is specified). The higher the value, the more residential proxy traffic the actor will consume!",
      "minimum": 1,
      "maximum": 100,
      "default": 2
    },
    "maxSessionsPerRegion": {
      "title": "Max sessions per region",
      "type": "integer",
      "description": "Maximum number of residential proxy sessions to maintain and keep alive for a single postal code or DMA (whichever is specified). The higher the value, the more residential proxy traffic the actor will consume!",
      "minimum": 1,
      "maximum": 100,
      "default": 5
    },
    "keyValueStoreName": {
      "title": "Key-value store name",
      "type": "string",
      "description": "Maximum number of residential proxy sessions to maintain and keep alive for a single postal code or DMA (whichever is specified). The higher the value, the more residential proxy traffic the actor will consume!",
      "editor": "textfield"
    },
    "recordKey": {
      "title": "Key-value store name",
      "type": "string",
      "description": "Maximum number of residential proxy sessions to maintain and keep alive for a single postal code or DMA (whichever is specified). The higher the value, the more residential proxy traffic the actor will consume!",
      "editor": "textfield",
      "default": "RESIDENTIAL_PROXY_SESSION_POOL"
    }
  },
